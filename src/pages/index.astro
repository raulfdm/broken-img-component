---
  import { Picture, Image } from '@astrojs/image/components';

  let imgSrc = 'https://www.google.com/images/branding/googlelogo/1x/googlelogo_color_272x92dp.png'
  let width = 272
  let height = 92
  let alt = 'Google Logo'
---

<img
  src={imgSrc}
  width={width}
  height={height}
  alt={alt}
/>
  
<Image
  src={imgSrc}
  width={width}
  height={height}
  alt={alt}
/>

<Picture
  fit="cover"
  src={imgSrc}
  loading="eager"
  widths={[200, 400, 640, 768, 1037, 1248]}
  sizes="(min-width: 1248px) 1248px, 100vw"
  aspectRatio={12 / 6}
  width={width}
  height={height}
  alt={alt}
/>

<Picture
  fit="cover"
  src={imgSrc}
  alt={alt}
  loading="eager"
  widths={[200]}
  aspectRatio={12 / 6}
  width={width}
  height={height}
/>