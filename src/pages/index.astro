---
  import { Picture, Image } from '@astrojs/image/components';
  let imgSrc = 'https://www.google.com/images/branding/googlelogo/1x/googlelogo_color_272x92dp.png'
---

<img
src={imgSrc}
/>
  
<Image
src={imgSrc}
width={200}
  height={300}
/>

<Picture
  fit="cover"
  src={imgSrc}
  alt={'random image'}
  loading="eager"
  widths={[200, 400, 640, 768, 1037, 1248]}
  sizes="(min-width: 1248px) 1248px, 100vw"
  aspectRatio={12 / 6}
  width={200}
  height={300}
/>

<Picture
  fit="cover"
  src={imgSrc}
  alt={'random image'}
  loading="eager"
  widths={[200]}
  aspectRatio={12 / 6}
  width={200}
  height={300}
/>